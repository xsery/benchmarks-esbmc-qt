#ifndef _QTEXTSTREAM_H
#define _QTEXTSTREAM_H

#include "../../cstdio"

class QTextStream {

private:
FILE *stdoutStream;

public:
	QTextStream(){}
        QTextStream( FILE *stdout){
	     __ESBMC_assert(stdout != NULL, "Invalid parameter");
	     stdoutStream = stdout;
	}
	~QTextStream(){}
};

#endif //_QTEXTSTREAM_H
